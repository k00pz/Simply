local notifSound = Instance.new("Sound",workspace)
notifSound.PlaybackSpeed = 1
notifSound.Volume = 0.35
notifSound.SoundId = "rbxassetid://1167155503"
notifSound.PlayOnRemove = true
notifSound:Destroy()


local SimpleUi = loadstring(game:HttpGet("https://raw.githubusercontent.com/laagginq/ui-libraries/main/simple-hub/src.lua"))()

local Main = SimpleUi:create('Universal')

local Menu = Main:Menu('Main')

----combat---

